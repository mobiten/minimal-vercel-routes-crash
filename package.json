{
  "name": "with-typescript-eslint-jest",
  "author": "@erikdstock",
  "license": "MIT",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "generate-i18n-keys": "node tasks/generate_i18n_keys.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "icons": "npx @svgr/cli -d icons icons --typescript --icon --no-dimensions --ignore-existing"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^0.3.1",
    "@prismicio/client": "^4.0.0",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/line-clamp": "^0.2.0",
    "@tailwindcss/typography": "^0.4.0",
    "autoprefixer": "^10.2.4",
    "clsx": "^1.1.1",
    "next": "^10.0.1",
    "postcss": "^8.2.7",
    "prismic-reactjs": "^1.3.3",
    "react": "^17.0.1",
    "react-div-100vh": "^0.5.6",
    "react-dom": "^17.0.1",
    "react-intl": "^5.9.2",
    "tailwindcss": "^2.0.3",
    "tailwindcss-filters": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@storybook/addon-actions": "^6.2.0-beta.5",
    "@storybook/addon-essentials": "^6.2.0-beta.5",
    "@storybook/addon-links": "^6.2.0-beta.5",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.2.0-beta.5",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-formatjs": "^2.9.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "lint-staged": "^10.0.10",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "typescript": "^4.1.2"
  }
}
